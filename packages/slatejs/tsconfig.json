{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["ESNext", "DOM"],
    "rootDir": ".",
    "noEmit": true,
    "target": "es6",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["./src/**/*", "/src/assets/*"]
}
