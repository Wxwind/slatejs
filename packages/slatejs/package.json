{
  "name": "slatejs",
  "version": "0.0.1",
  "description": "cinematic timeline editor for web",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "rollup --config",
    "lint:ts": "tsc -p ./tsconfig.json",
    "watch": "node scripts/watch.cjs"
  },
  "keywords": [
    "slate",
    "slatejs",
    "timeline",
    "animation",
    "editor"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rollup/plugin-url": "^8.0.2",
    "@svgr/rollup": "^8.1.0",
    "@types/node": "^20.8.3",
    "postcss": "^8.4.31",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.0.1",
    "@types/uuid": "^9.0.6",
    "classnames": "^2.3.2",
    "deer-engine": "workspace:*",
    "uuid": "^9.0.1",
    "zustand": "^4.4.4"
  }
}
