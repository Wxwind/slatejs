{
  "name": "slatejs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.8.0",
  "scripts": {
    "dev": "concurrently -n engine,canvas,website \"pnpm --filter deer-engine watch\" \"pnpm --filter deer-canvas watch\" \"pnpm --filter deer-website dev\"",
    "dev:bp": "concurrently -n bp,bp-website \"pnpm --filter deer-blueprint watch\" \"pnpm --filter deer-blueprint-example dev\"",
    "build": "pnpm --filter deer-website... build",
    "tsc": "pnpm --filter deer-website... lint:ts",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 -c eslint.config.js --fix",
    "preview": "pnpm --filter deer-website preview",
    "test": "jest"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.6",
    "canvas": "^2.11.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.26.1"
  }
}
