{
  "name": "slatejs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.8.0",
  "scripts": {
    "dev": "concurrently -n engine,website \"pnpm --filter deer-engine watch\" \"pnpm --filter website dev\"",
    "dev:eg": "pnpm --filter deer-engine watch",
    "build": "pnpm run build:eg && pnpm run build:website",
    "build:website": "pnpm --filter website build",
    "build:eg": "pnpm --filter deer-engine build",
    "lint:website": "pnpm --filter website lint:ts",
    "lint:eg": "pnpm --filter deer-engine lint:ts",
    "preview": "pnpm --filter website preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^3.25.3",
    "tslib": "^2.6.0",
    "typescript": "^5.3.3",
    "vite": "^4.3.9"
  }
}
