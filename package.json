  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.8.0",
  "scripts": {
    "dev": "concurrently -n engine,canvas,website \"pnpm --filter deer-engine watch\" \"pnpm --filter deer-canvas watch\" \"pnpm --filter website dev\"",
    "build": "pnpm --filter website... build",
    "build:website": "pnpm --filter website build",
    "build:eg": "pnpm --filter deer-engine build",
    "build:canvas": "pnpm --filter deer-canvas build",
    "lint:website": "pnpm --filter website... lint:ts",
    "lint:eg": "pnpm --filter deer-engine lint:ts",
    "lint:canvas": "pnpm --filter deer-canvas lint:ts",
    "preview": "pnpm --filter website preview",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2"
